  },
  "license": "MIT",
  "dependencies": {
    "@a-part/mongoose-pagination-plugin": "^1.0.0",
    "@nestjs/common": "^8.2.6",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/mongoose": "^9.0.2",
    "apollo-server-core": "^3.6.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "dataloader": "^2.0.0",
    "graphql": "^15.8.0",
    "graphql-scalars": "^1.14.1",
    "mongoose": "^6.1.8"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "test"
    ],
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src": "<rootDir>/src",
      "src/(.*)": "<rootDir>/src/$1"
    },
    "testTimeout": 20000
  }
}
